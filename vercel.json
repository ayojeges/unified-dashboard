{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://unified-dashboard.vercel.app"
  },
  "redirects": [
    {
      "source": "/signup",
      "destination": "/auth/register",
      "permanent": false
    },
    {
      "source": "/dashboard",
      "destination": "/",
      "permanent": false
    }
  ]
}